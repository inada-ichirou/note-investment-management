{
  "functions": {
    "api/auto-note.js": {
      "maxDuration": 300
    },
    "api/follow-articles.js": {
      "maxDuration": 300
    },
    "api/create-draft.js": {
      "maxDuration": 300
    },
    "api/publish-notes.js": {
      "maxDuration": 300
    },
    "api/like-notes.js": {
      "maxDuration": 300
    },
    "api/logs.js": {
      "maxDuration": 300
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
